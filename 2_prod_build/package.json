{
  "name": "demo_webpack",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fchastanet/demo_webpack.git"
  },
  "description": "webpack demonstration",
  "main": "index.js",
  "scripts": {
    "prod": "webpack --mode production --progress --config webpack.config.prod.js ",
    "prod-serve": "webpack serve --mode production --host 0.0.0.0 --disable-host-check --progress --config webpack.config.prod.js ",
    "dev": "webpack serve --mode development --host 0.0.0.0 --disable-host-check --progress --mode development --config webpack.config.dev.js",
    "bundle-analysis": "webpack --profile --config webpack.config.prod.js --json > stats.json && webpack-bundle-analyzer --host 0.0.0.0 stats.json",
    "bundle-analysis-dev": "webpack --profile --config webpack.config.dev.js --json > stats.json && webpack-bundle-analyzer --host 0.0.0.0 stats.json"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "vue": "^2.6.12",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime-corejs3": "^7.13.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.2",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
